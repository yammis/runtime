$(function () {
    $(".sa-gen").each(function () {
        $(this).click(function () {
            var id = $(this).next().val();
            $.post("page/packageGen", { areaId: id }, function (data) {
                if (data) {
                    location = location;
                    alert("生成成功!");

                }
            },"json")
        })
    })
})


